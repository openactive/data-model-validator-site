{
  "presets": ["env", "react"],
  "plugins": ["transform-class-properties"],
  "ignore": [],
  "env": {
    "development": {
      "plugins": ["inline-dotenv"]
    },
    "test": {
      "plugins": [
        [
          "import-redirect",
          {
            "root": "./spec/mocks",
            "redirect": {
              "\\.(scss|css|png|gif|jpg|jpeg)$": false,
              "\\.svg$": "./Component.mock.js"
            }
          }
        ]
      ]
    }
  }
}
    "style-loader": "^0.21.0",
    "sync-request": "^6.1.0",
    "typescript": "^5.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.1.9"
  }
